{
  "name": "udawa",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href ./ ",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron .",
    "dist-win": "electron-builder --win",
    "dist-linux": "electron-builder --linux"
  },
  "author": "John Doe <john@doe.com>",
  "productName": "UDAWA",
  "description": "The single app for multiple UDAWA Smart System device interface.",
  "private": true,
  "main": "main.js",
  "build": {
    "directories": {
        "output": "release",
        "buildResources": "dist"
    },
    "asar":false,
    "appId": "org.udawa.device",
    "win": {
        "target": "portable",
        "icon": "src/assets/img/logo.ico"
    },
    "linux": {
        "target": "deb",
        "icon": "src/assets/img/logo.png",
        "executableName": "udawa",
        "category": "GNOME;GTK;Network;InstantMessaging",
        "desktop": {
          "StartupWMClass": "udawa",
          "MimeType": "x-scheme-handler/udawa"
        },
        "artifactName": "udawa-${version}-${os}-${arch}.${ext}"
    }
  },
  "dependencies": {
    "@angular/animations": "^16.1.2",
    "@angular/cdk": "^16.1.2",
    "@angular/common": "^16.1.2",
    "@angular/compiler": "^16.1.2",
    "@angular/core": "^16.1.2",
    "@angular/forms": "^16.1.2",
    "@angular/material": "^16.1.2",
    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@capacitor/android": "^5.0.5",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/cli": "^5.0.5",
    "@capacitor/core": "^5.0.5",
    "@capacitor/ios": "^5.0.5",
    "@capacitor/network": "^5.0.5",
    "@ionic/angular": "^7.1.2",
    "chart.js": "^4.3.0",
    "chartist": "^1.3.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "crypto-js": "^4.1.1",
    "moment": "^2.29.4",
    "ng-chartist": "^8.1.0",
    "ng2-charts": "^4.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.1",
    "@angular/cli": "^16.1.1",
    "@angular/compiler-cli": "^16.1.2",
    "@capacitor/cli": "latest",
    "@types/jasmine": "~4.3.0",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
